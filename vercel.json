{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/assess",
      "dest": "api.py"
    },
    {
      "src": "/history",
      "dest": "api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
